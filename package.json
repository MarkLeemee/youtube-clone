{
    "name": "you-tube",
    "version": "1.0.0",
    "description": "JS study",
    "main": "init.js",
    "scripts": {
        "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
        "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
        "build:assets": "cd src && WEBPACK_ENV=production webpack",
        "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
        "copyAll": "cp -R src/static build && cp -R src/views build",
        "build": "npm run build:server && npm run build:assets && npm run copyAll",
        "prebuild": "rm -rf build",
        "tunnel": "lt --port 4000",
        "start": "PRODUCTION=true node build/init.js",
        "prestart": "npm run build"
    },
    "author": "Mark",
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.11.6",
        "@babel/node": "^7.10.5",
        "@babel/polyfill": "^7.10.4",
        "autoprefixer": "^9.8.6",
        "aws-sdk": "^2.747.0",
        "axios": "^0.20.0",
        "body": "^5.1.0",
        "body-parser": "^1.19.0",
        "connect-mongo": "^3.2.0",
        "cookie-parser": "^1.4.5",
        "core-js": "^3.6.5",
        "css-loader": "^1.0.1",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-flash": "0.0.2",
        "express-session": "^1.17.1",
        "get-blob-duration": "^1.1.2",
        "helmet": "^4.1.0",
        "localtunnel": "^2.0.0",
        "me": "^0.3.0",
        "mongoose": "^5.10.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.2",
        "multer-s3": "^2.9.0",
        "ngrok": "^3.2.7",
        "node-sass": "^4.14.1",
        "parser": "^0.1.4",
        "passport": "^0.4.1",
        "passport-facebook": "^3.0.0",
        "passport-github": "^1.1.0",
        "passport-local": "^1.0.0",
