//- 레이아웃 가져오기
extends layouts/main
include mixins/videoBlock

//- 레이아웃에서 block content 부분을 작성할 수 있다
block content
    .home-videos
        each item in videos
            +videoBlock({
                id:item.id,
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl
            })